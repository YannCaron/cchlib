<project xmlns="http://maven.apache.org/POM/4.0.0"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://maven.apache.org/POM/4.0.0
  http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <parent>
    <groupId>com.googlecode.cchlib</groupId>
    <artifactId>cchlib</artifactId>
    <version>4.2-SNAPSHOT</version>
  </parent>

  <artifactId>xcchlib-googlecontact</artifactId>
  <packaging>jar</packaging>
  <name>xcchlib-googlecontact</name>

  <!-- Global properties -->
  <properties>
    <!-- Javadoc is build and did not break the build if any error -->
    <maven.javadoc.plugin.fail.on.error>false</maven.javadoc.plugin.fail.on.error>
  </properties>

  <repositories>
  
    <repository>
      <id>cchlib-repository-release</id>
      <name>The cchlib Repository</name>
      <!-- TODO : use a public URL -->
      <url>http://framboise01/maven/release/</url>
      <snapshots>
        <enabled>false</enabled>
      </snapshots>
    </repository>

    <repository>
      <id>cchlib-repository-snapshot</id>
      <name>The cchlib SNAPSHOT Repository</name>
      <!-- TODO : use a public URL -->
      <url>http://framboise01/maven/snapshot/</url>
      <snapshots>
        <enabled>true</enabled>
      </snapshots>
    </repository>

  </repositories>

  <dependencies>

    <dependency>
      <groupId>com.googlecode.cchlib</groupId>
      <artifactId>cchlib-core</artifactId>
      <version>4.2-SNAPSHOT</version>
      <type>jar</type>
      <scope>compile</scope>
    </dependency>

    <dependency>
        <groupId>com.googlecode.cchlib</groupId>
        <artifactId>cchlib-io</artifactId>
        <version>4.2-SNAPSHOT</version>
    </dependency>

    <dependency><!-- Required by PhoneRecordSorterApp -->
      <groupId>com.googlecode.cchlib</groupId>
      <artifactId>cchlib-swing</artifactId>
      <version>4.2-SNAPSHOT</version>
    </dependency>

    <dependency>
        <groupId>org.codehaus.groovy</groupId>
        <artifactId>groovy-json</artifactId>
        <version>2.4.0-beta-2</version>
    </dependency>

    <dependency>
      <groupId>com.googlecode.jcsv</groupId>
      <artifactId>jcsv</artifactId>
      <version>1.4.0</version>
    </dependency>

    <!-- flexjson.JSONDeserializer, flexjson.JSONSerializer -->
    <dependency><!-- Required by PhoneRecordSorterApp -->
      <groupId>net.sf.flexjson</groupId>
      <artifactId>flexjson</artifactId>
      <version>3.2</version>
    </dependency>

  </dependencies>

  <build>

    <resources>
    </resources>

    <extensions>
    </extensions>

    <plugins>
    </plugins>

  </build>

</project>
