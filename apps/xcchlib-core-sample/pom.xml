<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://maven.apache.org/POM/4.0.0
  http://maven.apache.org/maven-v4_0_0.xsd">
  <modelVersion>4.0.0</modelVersion>
  <parent>
    <groupId>com.googlecode.cchlib</groupId>
    <artifactId>cchlib</artifactId>
    <version>4.2-SNAPSHOT</version>
  </parent>

  <artifactId>xcchlib-core-sample</artifactId>
  <packaging>jar</packaging>
  <name>xcchlib-core-sample</name>
  <description>Some samples for cchlib-core</description>
  <url>https://github.com/cClaude/cchlib/</url>

  <!-- Global properties -->
  <properties>
    <!-- Javadoc is build and break the build if any error -->
    <maven.javadoc.plugin.fail.on.error>true</maven.javadoc.plugin.fail.on.error>
  </properties>

  <dependencies>

    <dependency>
      <groupId>com.googlecode.cchlib</groupId>
      <artifactId>cchlib-core</artifactId>
      <version>4.2-SNAPSHOT</version>
      <type>jar</type>
      <scope>compile</scope>
    </dependency>

    <dependency>
      <groupId>com.googlecode.cchlib</groupId>
      <artifactId>cchlib-io</artifactId>
      <version>4.2-SNAPSHOT</version>
      <type>jar</type>
      <scope>compile</scope>
    </dependency>

    <dependency>
      <groupId>com.googlecode.cchlib</groupId>
      <artifactId>cchlib-swing</artifactId>
      <version>4.2-SNAPSHOT</version>
    </dependency>

    <dependency>
      <groupId>com.googlecode.cchlib</groupId>
      <artifactId>cchlib-i18n</artifactId>
      <version>4.2-SNAPSHOT</version>
    </dependency>

    <dependency>
      <groupId>com.googlecode.cchlib</groupId>
      <artifactId>cchlib-net</artifactId>
      <version>4.2-SNAPSHOT</version>
    </dependency>

    <dependency>
      <groupId>com.googlecode.cchlib</groupId>
      <artifactId>cchlib-unit</artifactId>
      <version>4.2-SNAPSHOT</version>
      <scope>test</scope>
    </dependency>

    <dependency><!-- Use by MyClassSerializationOkUsingStaticAndSLF4J -->
      <groupId>org.slf4j</groupId>
      <artifactId>jcl-over-slf4j</artifactId>
      <version>1.7.7</version>
    </dependency>

    <dependency><!-- Use by MyClassSerializationOkUsingStaticAndSLF4J -->
      <groupId>org.slf4j</groupId>
      <artifactId>slf4j-log4j12</artifactId>
      <version>1.7.7</version>
    </dependency>

  </dependencies>

  <build>

    <plugins>
    </plugins>

    <extensions>
    </extensions>

  </build>

</project>
